/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include <mips/qca/ar9331.dtsi>
#include "ar9331.dtsi"

/ {
    model = "General AR9331";
    compatible = "general,ar9331";

    chosen {
        environment@0 {
            compatible = "barebox,environment";
            device-path = &spiflash, "partname:barebox-environment";
        };
    };

    aliases {
        serial0 = &uart;
        spiflash = &spiflash;
    };

    leds {
        compatible = "gpio-leds";

        test {
            label = "test";
            gpios = <&gpio 0 GPIO_ACTIVE_HIGH>;
            default-state = "off";
        };
    };
};

&uart {
    status = "okay";
};

&gpio {
    status = "okay";
};

&spi {
    num-chipselects = <1>;
    status = "okay";

    /* Spansion S25FL032PIF SPI flash */
    spiflash: s25sl032p@0 {
        #address-cells = <1>;
        #size-cells = <1>;
        compatible = "spansion,s25sl032p", "jedec,spi-nor";
        spi-max-frequency = <104000000>;
        reg = <0>;
    };
};

&spiflash {
    partition@0 {
        label = "barebox";
        reg = <0 0x20000>;  /* 128K */
    };

    partition@20000 {
        label = "barebox-environment";
        reg = <0x20000 0x10000>;    /* 64K */
    };

    partition@30000 {
        label = "firmware";
        reg = <0x30000 0x500000>;   /* 5M */
    };

    partition@530000 {
        label = "art";
        reg = <0x530000 0x10000>;   /* 64K */
    };
};

&eth1 {
    status = "okay";
};
